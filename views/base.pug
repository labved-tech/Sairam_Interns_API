		include partials/_extras/offcanvas/quick-user.pug
		include partials/_extras/offcanvas/quick-cart.pug
		include partials/_extras/offcanvas/quick-panel.pug
		include partials/_extras/chat.pug
		include partials/_extras/scrolltop.pug
		include partials/_extras/toolbar.pug
		include partials/_extras/offcanvas/demo-panel.pug

		// begin::Global Theme Bundle(used by all pages)
		script(src='/plugins/global/plugins.bundle.js')
		script(src='/plugins/custom/prismjs/prismjs.bundle.js')
		script(src='/js/scripts.bundle.js')
		// end::Global Theme Bundle
		
		// begin::Page Scripts(used by this page)
		script(src='/js/KTAppSettings.js')
		script(src='/plugins/global/axios/axios.min.js')
