extends ./../../base.pug

block css-files

block js-files
    script(src='/js/pages/crud/myPages/event/eventEntries.js')


block content
    #kt_content(class='content d-flex flex-column flex-column-fluid')
        include ./../../partials/_subheader/subheader-v1.pug
        // begin::Entry
        div(class='d-flex flex-column-fluid')
            // begin::Container
            div(class='container')
                        // begin::Card
                        div(class="card card-custom gutter-b")
                            div(class="card-header flex-wrap border-0 pt-6 pb-0")
                                div(class="card-title")
                                    h3(class="card-label")
                                        | Event Entries
                                        span(class="d-block text-muted pt-2 font-size-sm") ajax row selection and group actions
                                div(class="card-toolbar")
                                    // begin::Dropdown
                                    div(class='dropdown dropdown-inline mr-2')
                                        button(type='button' class='btn btn-light-primary font-weight-bolder dropdown-toggle' data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
                                            i(class='fas fa-file-export')
                                            |  Export
                                        // begin::Dropdown Menu
                                        div(class="dropdown-menu dropdown-menu-sm dropdown-menu-right")
                                            // begin::Navigation
                                            ul(class="navi flex-column navi-hover py-2")
                                                li(class="navi-header font-weight-bolder text-uppercase font-size-sm text-primary pb-2")
                                                    | Choose an option:
                                                li(class="navi-item")
                                                    a(href="#", class="navi-link")
                                                        span(class="navi-icon")
                                                            i(class="fas fa-print")
                                                        span(class="navi-text") Print
                                                li(class="navi-item")
                                                    a(href="#", class="navi-link")
                                                        span(class="navi-icon")
                                                            i(class="fas fa-copy")
                                                        span(class="navi-text") Copy
                                                li(class="navi-item")
                                                    a(href="#", class="navi-link")
                                                        span(class="navi-icon")
                                                            i(class="fas fa-file-excel")
                                                        span(class="navi-text") Excel
                                                li(class="navi-item")
                                                    a(href="#", class="navi-link")
                                                        span(class="navi-icon")
                                                            i(class="fas fa-file-csv")
                                                        span(class="navi-text") CSV
                                                li(class="navi-item")
                                                    a(href="#", class="navi-link")
                                                        span(class="navi-icon")
                                                            i(class="fas fa-file-pdf")
                                                        span(class="navi-text") PDF
                                            // end::Navigation
                                        // end::Dropdown Menu
                                    // end::Dropdown
                                    // begin::Button
                                    // Button trigger modal
                                    button#btnOpenEeModal(type='button' class='btn btn-primary font-weight-bolder btnAdd')
                                        | Add New

                            div(class="card-body")
                                // begin::Search Form
                                div(class="mb-7")
                                    div(class="row align-items-center")
                                        div(class="col-lg-9 col-xl-8")
                                            div(class="row align-items-center")
                                                div(class="col-md-4 my-2 my-md-0")
                                                    div(class="input-icon")
                                                        input#tableEe_search_query_2(type="text", class="form-control", placeholder="Search...")
                                                        span
                                                            i(class="flaticon2-search-1 text-muted")
                                                div(class="col-md-4 my-2 my-md-0")
                                                    div(class="d-flex align-items-center")
                                                        label(class="mr-3 mb-0 d-none d-md-block") Status:
                                                        select#tableEe_search_status_2(class="form-control")
                                                            option(value="") All
                                                            option(value="1") Pending
                                                            option(value="2") Delivered
                                                            option(value="3") Canceled
                                                            option(value="4") Success
                                                            option(value="5") Info
                                                            option(value="6") Danger
                                                div(class="col-md-4 my-2 my-md-0")
                                                    div(class="d-flex align-items-center")
                                                        label(class="mr-3 mb-0 d-none d-md-block") Type:
                                                        select#tableEe_search_type_2(class="form-control")
                                                            option(value="") All
                                                            option(value="1") Online
                                                            option(value="2") Retail
                                                            option(value="3") Direct
                                        div(class="col-lg-3 col-xl-4 mt-5 mt-lg-0")
                                            a(href="#", class="btn btn-light-primary px-6 font-weight-bold")
                                                | Search
                                // end::Search Form
                                // begin: Selected Rows Group Action Form
                                #tableEe_group_action_form_2(class="mt-10 mb-5 collapse")
                                    div(class="d-flex align-items-center")
                                        div(class="font-weight-bold text-danger mr-3")
                                            | Selected 
                                            span#tableEe_selected_records_2 0
                                            |  records:
                                        div(class="dropdown mr-2")
                                            button(type="button", class="btn btn-primary btn-sm dropdown-toggle", data-toggle="dropdown")
                                                | Update status
                                            div(class="dropdown-menu dropdown-menu-sm")
                                                ul(class="nav nav-hover flex-column")
                                                    li(class="nav-item")
                                                        a(href="#", class="nav-link")
                                                            span(class="nav-text") Pending
                                                    li(class="nav-item")
                                                        a(href="#", class="nav-link")
                                                            span(class="nav-text") Delivered
                                                    li(class="nav-item")
                                                        a(href="#", class="nav-link")
                                                            span(class="nav-text") Canceled
                                        button#btnDeleteAll(class="btn btn-sm btn-danger mr-2 btnDeleteAll", type="button")
                                            | Delete All
                                        button(class="btn btn-sm btn-success", type="button", data-toggle="modal", data-target="#tableEe_fetch_modal_2")
                                            | Fetch Selected Records
                                // end: Selected Rows Group Action Form
                                // begin: Datatable
                                #tableEe(class="datatable datatable-bordered datatable-head-custom")
                                // end: Datatable
                            // end::Card
                        // end::Card

                        #modalEe(class='modal fade' data-backdrop='static' data-keyboard='true' tabindex='-1' role='dialog' aria-labelledby='EeModalLabel' aria-hidden='true')
                            div(class='modal-dialog modal-dialog-top modal-xl modal-dialog-scrollable' role='document')
                                div(class='modal-content')
                                    // begin::Form
                                    form#formEe(class='form')
                                        div(class='modal-header')
                                            h5(class='modal-title') Add New
                                            button(type='button' class='close btnClose' data-dismiss='modal' aria-label='Close')
                                                i(aria-hidden='true' class='ki ki-close')
                                        div(class='modal-body')
                                            div(data-scroll='true' data-height='350')
                                                // EeId - hidden
                                                div(class= 'form-group row')
                                                    input#eeId(type='hidden' name='eeId')

                                        // eePriority
                                        div(class='form-group row')
                                            label(class='col-3 col-form-label form-control-label text-right') Priority
                                            div(class='col-4')
                                                input#eePriority(type='text' class='form-control bootstrap-touchspin-vertical-btn' value='' name='eePriority' placeholder='1')

                                        // eeExpires
                                        div(class='form-group row')
                                            label(class='col-3 col-form-label form-control-label text-right') Expires In
                                                span(class='text-danger')  *
                                            div(class=' col-4')
                                                select#eeExpires(class='form-control select2' name='eeExpires')
                                                    option(value='1d') Daily
                                                    option(value='7d') Weekly
                                                    option(value='30d') Monthly
                                                    option(value='1y') Yearly
                                                span(class='form-text text-muted') Lorem Ipsum is simply dummy text of the printing and typesetting industry.

                                        // eeType
                                        div(class='form-group row')
                                            label(class='col-3 col-form-label form-control-label text-right') Type
                                            div(class=' col-4')
                                                select#eeType(class='form-control select2' name='eeType')
                                                    option(value='AK') Alaska
                                                    option(value='HI') Hawaii
                                                    option(value='CA') California
                                                    option(value='NV') Nevada
                                                    option(value='OR') Oregon
                                                    option(value='WA') Washington
                                                    option(value='AZ') Arizona
                                                    option(value='CO') Colorado
                                                    option(value='ID') Idaho
                                                    option(value='MT') Montana
                                                    option(value='NE') Nebraska
                                                    option(value='NM') New Mexico
                                                    option(value='ND') North Dakota
                                                    option(value='UT') Utah
                                                    option(value='WY') Wyoming
                                                    option(value='AL') Alabama
                                                    option(value='AR') Arkansas
                                                    option(value='IL') Illinois
                                                    option(value='IA') Iowa
                                                    option(value='KS') Kansas
                                                    option(value='KY') Kentucky
                                                    option(value='LA') Louisiana
                                                    option(value='MN') Minnesota
                                                    option(value='MS') Mississippi
                                                    option(value='MO') Missouri
                                                    option(value='OK') Oklahoma
                                                    option(value='SD') South Dakota
                                                    option(value='TX') Texas
                                                    option(value='TN') Tennessee
                                                    option(value='WI') Wisconsin
                                                    option(value='CT') Connecticut
                                                    option(value='DE') Delaware
                                                    option(value='FL') Florida
                                                    option(value='GA') Georgia
                                                    option(value='IN') Indiana
                                                    option(value='ME') Maine
                                                    option(value='MD') Maryland
                                                    option(value='MA') Massachusetts
                                                    option(value='MI') Michigan
                                                    option(value='NH') New Hampshire
                                                    option(value='NJ') New Jersey
                                                    option(value='NY') New York
                                                    option(value='NC') North Carolina
                                                    option(value='OH') Ohio
                                                    option(value='PA') Pennsylvania
                                                    option(value='RI') Rhode Island
                                                    option(value='SC') South Carolina
                                                    option(value='VT') Vermont
                                                    option(value='VA') Virginia
                                                    option(value='WV') West Virginia
                                                span(class='form-text text-muted') Lorem Ipsum is simply dummy text of the printing and typesetting industry.

                                        h3(class=' text-dark font-weight-bold mb-10') Meta
                                        div(class='form-group row')

                                        // eeBoundary
                                        div(class='form-group row mb-6')
                                            label(class='col-3 col-form-label form-control-label text-right') boundary
                                            div(class='col-6 ')
                                                div(class='row align-items-center')
                                                    div(class='col-4')
                                                        input#eeBoundaryTxt(type='text' class='form-control' name='eeBoundaryTxt' placeholder='Quantity')
                                                    div(class='col-8')
                                                        #eeBoundarySlider(class='nouislider-drag-danger')
                                                span(class='form-text text-muted') Lorem Ipsum is simply dummy text of the printing and typesetting industry.

                                        div(class='modal-footer')
                                            button#btnResetEeForm(type='reset' class='btn btn-light-primary font-weight-bold btnReset') Reset
                                            button#btnAddNewEeFormSubmitButton(class='btn btn-primary font-weight-bold btnAdd') Add
                                            button#btnSaveEeFormSubmitButton(class='btn btn-primary font-weight-bold btnSave') Save changes
