block content
    #kt_content(class='content d-flex flex-column flex-column-fluid')
        include ./../../partials/_subheader/subheader-v1.pug
        // begin::Entry
        div(class='d-flex flex-column-fluid')
            // begin::Container
            div(class='container')
                // begin::Card
                div(class='card card-custom')
                    div(class='card-header')
                        h3(class='card-title')
                            | Example Form
                    div(class='card-body')
                        // begin::Form
                        form#kt_form_1(class='form')
                            div(class='row')
                                div(class='col-xl-2')
                                div(class='col-xl-8')
                                    div(class='my-5')

                                        h3(class=' text-dark font-weight-bold mb-10') Textbox
                                        // Name
                                        div(class='form-group row')
                                            label(class='col-form-label col-lg-3 col-sm-12 form-control-label text-right ') Text
                                                span(class='text-danger')  *
                                            div(class='col-lg-6 col-md-9 col-sm-12')
                                                input#text(type='text' class='form-control' name='text' placeholder='Enter your email')
                                                span(class='form-text text-muted') We'll never share your email with anyone else.                                   
                                        // password
                                        div(class='form-group row')
                                            label(class='col-3 col-form-label text-right form-control-label') Password
                                                span(class='text-danger')  *
                                            div(class='col-lg-4 col-md-9 col-sm-12')
                                                input#password(type='text' class='form-control' name='password' placeholder='password')
                                                span(class='form-text text-muted') minimum 8 characters                                   
                                        // Text Area
                                        div(class='form-group row mb-1')
                                            label(class='col-3 col-form-label text-right' for='exampleTextarea')
                                                | Textarea 
                                                span(class='text-danger') *
                                            div(class='col-lg-9 col-md-12 col-sm-12')
                                                textarea#exampleTextarea(class='form-control' name='exampleTextarea' rows='3')
                                                span(class='form-text text-muted') minimum 8 characters                                   

                                        // Typeahead : local data
                                        div(class='form-group row')
                                            label(class='col-form-label col-lg-3 col-sm-12 form-control-label text-right') Typeahead Basic Demo
                                            div(class='col-lg-4 col-md-9 col-sm-12')
                                                div(class='typeahead')
                                                    input#kt_typeahead_1(class='form-control' type='text' dir='ltr' name='kt_typeahead_1' placeholder='States of USA')
                                        // Typeahead : Remote data
                                        div(class='form-group row')
                                            label(class='col-form-label text-right col-lg-3 col-sm-12') Typeahead Remote Data (Suggestion Engine)
                                            div(class='col-lg-4 col-md-9 col-sm-12')
                                                div(class='typeahead')
                                                    input#kt_typeahead_2(class='form-control' type='text' dir='ltr'  name='kt_typeahead_2' placeholder='States of USA')
