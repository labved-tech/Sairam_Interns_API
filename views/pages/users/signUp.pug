doctype html

html(lang='en')
	// begin::Head
	head
		base(href='../../../../')
		meta(charset='utf-8')
		title Metronic | Sign Up
		meta(name='description' content='Singin page example')
		meta(name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no')
		// begin::Fonts
		link(rel='stylesheet' href='https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700')
		// end::Fonts
		// begin::Page Custom Styles(used by this page)
		link(href='css/pages/login/login-3.css' rel='stylesheet' type='text/css')
		// end::Page Custom Styles
		// begin::Global Theme Styles(used by all pages)
		link(href='plugins/global/plugins.bundle.css' rel='stylesheet' type='text/css')
		link(href='plugins/custom/prismjs/prismjs.bundle.css' rel='stylesheet' type='text/css')
		link(href='css/style.bundle.css' rel='stylesheet' type='text/css')
		// end::Global Theme Styles
		// begin::Layout Themes(used by all pages)
		link(href='css/themes/layout/header/base/light.css' rel='stylesheet' type='text/css')
		link(href='css/themes/layout/header/menu/light.css' rel='stylesheet' type='text/css')
		link(href='css/themes/layout/brand/dark.css' rel='stylesheet' type='text/css')
		link(href='css/themes/layout/aside/dark.css' rel='stylesheet' type='text/css')
		// end::Layout Themes
		link(rel='shortcut icon' href='media/logos/favicon.ico')
	// end::Head
	// begin::Body
	body#kt_body(class='header-fixed header-mobile-fixed subheader-enabled subheader-fixed subheader-mobile-fixed aside-enabled aside-fixed aside-minimize-hoverable page-loading')
		// begin::Main
		div(class='d-flex flex-column flex-root')
			// begin::Login
			#kt_login(class='login login-3 wizard d-flex flex-column flex-lg-row flex-column-fluid wizard')
				// begin::Aside
				div(class='login-aside d-flex flex-column flex-row-auto')
					// begin::Aside Top
					div(class='d-flex flex-column-auto flex-column pt-15 px-30')
						// begin::Aside header
						a(href='#' class='login-logo py-6')
							img(src='media/logos/logo-1.png' class='max-h-70px' alt='')
						// end::Aside header
						
				// end::Aside
				// begin::Content
				div(class='login-content flex-column-fluid d-flex flex-column p-10')
					// begin::Top
					div(class='text-right d-flex justify-content-center')
						div(class='top-signup text-right d-flex justify-content-end pt-5 pb-lg-0 pb-10')
							span(class='font-weight-bold text-muted font-size-h4') Having issues?
							a#kt_login_signup(href='javascript:;' class='font-weight-bolder text-primary font-size-h4 ml-2') Get Help
					// end::Top
					// begin::Wrapper
					div(class='d-flex flex-row-fluid flex-center')
						// begin::Signin
						div(class='login-form login-form-signup')
							// begin::Form
							form#signUpForm(class='form')
								// begin::Title
								div(class='pb-10 pb-lg-15')
									h3(class='font-weight-bolder text-dark display5') Create Account
									div(class='text-muted font-weight-bold font-size-h4')
										| Already have an Account ?
										a(href='custom/pages/login/login-3/signin.html' class='text-primary font-weight-bolder')  Sign In
								// begin::Title
								// begin::Form Group
								div(class='form-group')
									label(class='font-size-h6 font-weight-bolder text-dark') Name
									input#signUpName(type='text' class='form-control h-auto py-7 px-6 border-0 rounded-lg font-size-h6' name='signUpName' placeholder='Name' value='')
								// end::Form Group
								// begin::Form Group
								div(class='form-group')
									label(class='font-size-h6 font-weight-bolder text-dark') Email Address
									input#signUpEmail(type='text' class='form-control h-auto py-7 px-6 border-0 rounded-lg font-size-h6' name='signUpEmail' placeholder='' value='')
								// end::Form Group
								// begin::Form Group
								div(class='form-group')
									label(class='font-size-h6 font-weight-bolder text-dark') Username
									input#signUpUsername(type='text' class='form-control h-auto py-7 px-6 border-0 rounded-lg font-size-h6' name='signUpUsername' placeholder='ex. user1' value='')
									span(class='form-text text-muted') Choose a username to go with this account
								// end::Form Group
								// begin::Form Group
								div(class='form-group row')
									div(class='col-lg-6')
										label(class='font-size-h6 font-weight-bolder text-dark') Password
										input#signUpPassword(type='password' class='form-control h-auto py-7 px-6 border-0 rounded-lg font-size-h6' name='signUpPassword' placeholder='' value='')
										span(class='form-text text-muted') minimum 8 characters    
									div(class='col-lg-6')
										label(class='font-size-h6 font-weight-bolder text-dark') Confirm Password
										input#signUpPasswordConfirm(type='password' class='form-control h-auto py-7 px-6 border-0 rounded-lg font-size-h6' name='signUpPasswordConfirm'  placeholder='' value='')
										span(class='form-text text-muted') minimum 8 characters    
								// end::Form Group
								// begin::Form Group
								div(class='form-group')
									label(class='font-size-h6 font-weight-bolder text-dark') Mobile Number
									input#signUpMobileNo(type='text' class='form-control h-auto py-7 px-6 border-0 rounded-lg font-size-h6' name='signUpMobileNo'  placeholder='' value='')
								// end::Form Group

								// begin::Form Group
								div(class='form-group')
									label(class='font-size-h6 font-weight-bolder text-dark') Date of Birth
										div(class='input-group date')
											input#signUpDob(type='text' class='form-control h-auto  py-7 px-6 border-0 rounded-lg font-size-h6' name='signUpDob' readonly='' placeholder='Select date')
											div(class='input-group-append')
												span(class='input-group-text')
													i(class='la la-calendar-check-o')
								// end::Form Group
								div(class='row text-right')
									div(class='col-lg-9 ml-lg-auto')
										button#signUpSubmitButton(type='submit' class='btn btn-primary font-weight-bold mr-2') Submit
										button(type='reset' class='btn btn-light-primary font-weight-bold') Cancel
							// end::Form
						// end::Signin
					// end::Wrapper
				// end::Content
			// end::Login
		// end::Main
		// begin::Global Theme Bundle(used by all pages)
		script(src='plugins/global/plugins.bundle.js')
		script(src='plugins/custom/prismjs/prismjs.bundle.js')
		script(src='js/scripts.bundle.js')
		// end::Global Theme Bundle
		// begin::Page Scripts(used by this page)
		script(src='/js/KTAppSettings.js')
		script(src='/plugins/global/axios/axios.min.js')
		script(src='/js/pages/crud/myForm/users/login-3.js')
		// end::Page Scripts
	// end::Body
