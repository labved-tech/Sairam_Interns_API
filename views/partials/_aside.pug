//- begin::Aside
#kt_aside(class='aside aside-left aside-fixed d-flex flex-column flex-row-auto')
	//- begin::Brand
	#kt_brand(class='brand flex-column-auto')
		//- begin::Logo
		a(href='index.html' class='brand-logo')
			img(alt='Logo' src='media/logos/logo-light.png')
		//- end::Logo
		//- begin::Toggle
		button#kt_aside_toggle(class='brand-toggle btn btn-sm px-0')
			span(class='svg-icon svg-icon svg-icon-xl')
				//- begin::Svg Icon | path:assets/media/svg/icons/Navigation/Angle-double-left.svg
				svg(xmlns='http://-www.w3.org/2000/svg' xmlns:xlink='http://-www.w3.org/1999/xlink' width='24px' height='24px' viewbox='0 0 24 24' version='1.1')
					g(stroke='none' stroke-width='1' fill='none' fill-rule='evenodd')
						polygon(points='0 0 24 0 24 24 0 24')
						path(d='M5.29288961,6.70710318 C4.90236532,6.31657888 4.90236532,5.68341391 5.29288961,5.29288961 C5.68341391,4.90236532 6.31657888,4.90236532 6.70710318,5.29288961 L12.7071032,11.2928896 C13.0856821,11.6714686 13.0989277,12.281055 12.7371505,12.675721 L7.23715054,18.675721 C6.86395813,19.08284 6.23139076,19.1103429 5.82427177,18.7371505 C5.41715278,18.3639581 5.38964985,17.7313908 5.76284226,17.3242718 L10.6158586,12.0300721 L5.29288961,6.70710318 Z' fill='#000000' fill-rule='nonzero' transform='translate(8.999997, 11.999999) scale(-1, 1) translate(-8.999997, -11.999999)')
						path(d='M10.7071009,15.7071068 C10.3165766,16.0976311 9.68341162,16.0976311 9.29288733,15.7071068 C8.90236304,15.3165825 8.90236304,14.6834175 9.29288733,14.2928932 L15.2928873,8.29289322 C15.6714663,7.91431428 16.2810527,7.90106866 16.6757187,8.26284586 L22.6757187,13.7628459 C23.0828377,14.1360383 23.1103407,14.7686056 22.7371482,15.1757246 C22.3639558,15.5828436 21.7313885,15.6103465 21.3242695,15.2371541 L16.0300699,10.3841378 L10.7071009,15.7071068 Z' fill='#000000' fill-rule='nonzero' opacity='0.3' transform='translate(15.999997, 11.999999) scale(-1, 1) rotate(-270.000000) translate(-15.999997, -11.999999)')
				//- end::Svg Icon
		//- end::Toolbar
	//- end::Brand
	//- begin::Aside Menu
	#kt_aside_menu_wrapper(class='aside-menu-wrapper flex-column-fluid')
		//- begin::Menu Container
		#kt_aside_menu(class='aside-menu my-4' data-menu-vertical='1' data-menu-scroll='1' data-menu-dropdown-timeout='500')
			// begin::Menu Nav
			ul(class='menu-nav')
				li(class='menu-item menu-item-active' aria-haspopup='true')
					a(href='/dashboard' class='menu-link')
						i(class='menu-icon flaticon2-poll-symbol')
						span(class='menu-text') Dashboard
				-var sectionNo = sectionItems.length
				////-console.log(`No of section :${sectionNo}`)
				-for(x=0;x<sectionNo;x++)
					li(class='menu-section')
						h4(class='menu-text')= sectionItems[x].sectionName
						i(class='menu-icon ki ki-bold-more-hor icon-md')
					-var menuItemsNo = sectionItems[x].menuItemsNo
					//-console.log(`No of Menu Items :${menuItemsNo}`)
					-if(menuItemsNo)
						-for(let i=0;i<menuItemsNo;i++)
							li(class='menu-item menu-item-submenu' aria-haspopup='true' data-menu-toggle='hover')
								a(href='/' + sectionItems[x].menuItems[i].route class='menu-link menu-toggle') 
									i(class='menu-icon flaticon2-box-1')
									span(class='menu-text')= sectionItems[x].menuItems[i].name
									-var subItems1No = sectionItems[x].menuItems[i].subItems1No
									//-console.log(`No of subItems1 :${subItems1No}`)
									-if(subItems1No)
										i(class='menu-arrow')
								div(class='menu-submenu')
										i(class='menu-arrow')
										ul(class='menu-subnav')
											-for(let j=0;j<subItems1No;j++)
												li(class='menu-item menu-item-submenu' aria-haspopup='true' data-menu-toggle='hover')
													a(href='/' + sectionItems[x].menuItems[i].route + '/' + sectionItems[x].menuItems[i].subItems1[j].route class='menu-link menu-toggle')
														i(class='menu-bullet menu-bullet-line')
															span
														span(class='menu-text')= sectionItems[x].menuItems[i].subItems1[j].name
														-var subItems2No = sectionItems[x].menuItems[i].subItems1[j].subItems2No
														//-console.log(`No of subItems2 :${subItems2No}`)
														-if(subItems2No)
															i(class='menu-arrow')
													div(class='menu-submenu')
															i(class='menu-arrow')
															ul(class='menu-subnav')
																-for(k=0;k<subItems2No;k++)
																	li(class='menu-item' aria-haspopup='true')
																		a(href='/' + sectionItems[x].menuItems[i].route + '/' + sectionItems[x].menuItems[i].subItems1[j].route +  '/' + sectionItems[x].menuItems[i].subItems1[j].subItems2[k].route class='menu-link')
																			i(class='menu-bullet menu-bullet-dot')
																				span
																			span(class='menu-text')= sectionItems[x].menuItems[i].subItems1[j].subItems2[k].name


				// end::Menu Nav
			// end::Menu Container
		// end::Aside Menu
	// end::Aside